{
  "buildCommand": "cd usdx/frontend && npm install && npm run build",
  "outputDirectory": "usdx/frontend/.next",
  "devCommand": "cd usdx/frontend && npm run dev",
  "installCommand": "cd usdx/frontend && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
